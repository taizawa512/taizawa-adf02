{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "taizawa-adf02-git"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "'AzureDataLakeStorage1' の 'accountKey' のセキュリティで保護された文字列"
		},
		"AzureFunction1_functionKey": {
			"type": "secureString",
			"metadata": "'AzureFunction1' の 'functionKey' のセキュリティで保護された文字列"
		},
		"ABS001_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://taizawaadls01.blob.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://taizawaadls01.dfs.core.windows.net/"
		},
		"AzureFunction1_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://taizawa-functions-01.azurewebsites.net"
		},
		"AzureFunction1_properties_typeProperties_resourceId": {
			"type": "string",
			"defaultValue": "ssss"
		},
		"containerUri": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを含むストレージ アカウントの URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを格納するストレージ アカウントへの SAS トークン"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "taizawa-adf02-git_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureFunction1_functionKey": {
						"value": "[parameters('AzureFunction1_functionKey')]"
					},
					"ABS001_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ABS001_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureFunction1_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('AzureFunction1_properties_typeProperties_functionAppUrl')]"
					},
					"AzureFunction1_properties_typeProperties_resourceId": {
						"value": "[parameters('AzureFunction1_properties_typeProperties_resourceId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}